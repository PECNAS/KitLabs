<Window x:Class="WpfApp5.WindowTables"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp5"
        mc:Ignorable="d"
        Title="WindowTables" Height="450" Width="800"
         Background="#FF2E2E2E">
    <StackPanel>
        <Button x:Name="Button1" Height="55" Width="300"
        Content="Curator" FontSize="18" Background="#FFE9CB80" Click="Button1_Click" />
        <Button x:Name="Button2" Height="55" Width="300"
        Content="Student" FontSize="18" Background="#FFE9CB80" Click="Button2_Click" />
        <Button x:Name="Button3" Height="55" Width="300"
Content="Parents" FontSize="18" Background="#FFE9CB80" Click="Button3_Click" />
        <Button x:Name="Button4" Height="55" Width="300"
Content="Summary Ledger" FontSize="18" Background="#FFE9CB80" Click="Button4_Click" />
        <Button x:Name="Button5" Height="55" Width="300"
Content="Health Card" FontSize="18" Background="#FFE9CB80" Click="Button5_Click" />
    </StackPanel>
</Window>
